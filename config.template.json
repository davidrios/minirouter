{
  "interfaces": ["enp5s0", "wlan0"],
  "display": {
    "size": [128, 32],
    "font_size": 10
  },
  "output": "screen",
  "output_scale": 6,
  "refresh": 1,
  "buttons_server": {
    "address": "tcp://localhost:5555",
    "button_a": 5,
    "button_b": 6,
    "direction": -1
  },
  "logging": {
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
      "default": {
        "datefmt": "%Y%m%d_%H%M%S",
        "format": "%(asctime)s.%(msecs)03d:%(name)s:%(levelname)s:%(message)s"
      }
    },
    "handlers": {
      "console": {
        "level": "DEBUG",
        "class": "logging.StreamHandler",
        "formatter": "default"
      }
    },
    "loggers": {
      "": {
        "level": "WARNING",
        "handlers": ["console"]
      },
      "minirouter": {
        "level": "DEBUG",
        "handlers": ["console"],
        "propagate": false
      }
    }
  }
}
